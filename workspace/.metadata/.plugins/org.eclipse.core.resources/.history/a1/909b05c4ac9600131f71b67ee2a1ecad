package excel;

import java.util.ArrayList;
import java.util.HashMap;

public class Table {
	ArrayList<HashMap<Integer, Double>> distArray = new ArrayList<HashMap<Integer, Double>>();
	ArrayList<HashMap<Integer, Integer>> prevArray = new ArrayList<HashMap<Integer, Integer>>();
	
	private void addColumn(HashMap<Integer, Double> distMap, HashMap<Integer, Integer> prevMap){
		HashMap<Integer, Double> tmpDist = new HashMap<Integer,Double>(distMap);
		HashMap<Integer, Integer> tmpPrev = new HashMap<Integer, Integer>(prevMap);
		this.distArray.add(tmpDist);
		this.prevArray.add(prevMap);
	}
}
