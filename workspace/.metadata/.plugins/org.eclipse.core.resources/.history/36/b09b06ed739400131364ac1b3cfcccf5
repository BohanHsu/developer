package heap;

import graph.Node;

import java.util.ArrayList;

public class NaryTree {
	private int n;
	private ArrayList<Entry> array = null;
	
	public NaryTree(int n) {
		super();
		this.n = n;
	}
	
	private int getParentIndex(int index){
		return (index + 1) / 2 - 1;
	}
	
	public void makeQueue(ArrayList<Entry> list){
		this.array = list;
		
	}
}
