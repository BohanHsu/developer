package algo_probs;

public class Permutation {
	public void permutation(String s){
		
	}
	
	private void permutation(String prefix, String s){
		int len = s.length();
		for (int i = 0; i < len; i++){
			permutation(prefix + s.substring(i, i + 1), s.substring(0, i ) + s.substring(i+1,len));
		}
	}
}
