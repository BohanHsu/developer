package heap;

import graph.Node;

public class Entry  {
	Node value = null;
	double key = 0;
	
	public Entry(Node value, double key) {
		super();
		this.value = value;
		this.key = key;
	}

	public Node getValue() {
		return value;
	}

	public void setValue(Node value) {
		this.value = value;
	}

	public double getKey() {
		return key;
	}

	public void setKey(double key) {
		this.key = key;
	}
	
	
}
