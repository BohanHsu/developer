	private final String BEGINEDGE = "\\path[every node/.style={font=\\sffamily\\small}]";
	private final String END = "\\end{tikzpicture}";
	
	public GraphLatex(Graph g) {
		super();
		this.g = g;
	}
	
	public String toLatex(){
		 
	}
}

