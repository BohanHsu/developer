package heap;

import graph.Node;

public class Entry  {
	Node key = null;
	Double value = null;
	public Entry(Node key, Double value) {
		super();
		this.key = key;
		this.value = value;
	}
	public Node getKey() {
		return key;
	}
	public void setKey(Node key) {
		this.key = key;
	}
	public double getValue() {
		return value;
	}
	public void setValue(double value) {
		this.value = value;
	}
	
	
		
}
