package test;

import java.util.HashSet;

public class PlainTest {
	public static void main(String[] args) {
		HashSet<Integer> hs = new HashSet<Integer>();
		hs.add(1);
		hs.add(2);
		hs.add(3);
		hs.add(4);
		
		for (Integer integer : hs) {
			hs.add(integer*2);
		}
		
		System.out.println(hs);
	}
}
