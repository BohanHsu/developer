package heap;

import graph.Node;

public class Entry  {
	Node value = null;
	double key = 0;
	boolean infinity = false;
	public Entry(Node value, double key, boolean infinity) {
		super();
		this.value = value;
		this.key = key;
		this.infinity = infinity;
	}
	public Node getValue() {
		return value;
	}
	public void setValue(Node value) {
		this.value = value;
	}
	public double getKey() {
		return key;
	}
	public void setKey(double key) {
		this.key = key;
	}
	public boolean isInfinity() {
		return infinity;
	}
	public void setInfinity(boolean infinity) {
		this.infinity = infinity;
	}
	
		
}
