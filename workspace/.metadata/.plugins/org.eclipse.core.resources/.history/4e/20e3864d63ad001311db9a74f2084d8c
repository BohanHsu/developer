package q2;

import q3.Graph;

public class Q2 {
	public static void main(String[] args) {

		String[] edge = new String[] { "1,2,2", "1,3,2", "1,4,3", "2,5,2",
				"2,6,2", "3,7,2", "3,8,2", "4,6,3", "4,7,3", "5,9,2", "6,9,1",
				"7,9,1", "8,9,2" };
		Graph g = new Graph(edge);
		int tc = g.dinic(5, 2);
		System.out.println(tc);
	}
}
